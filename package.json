{
	"name": "bbf11ty",
	"version": "1.0.0",
	"description": "BBF website based off 11ty",
	"scripts": {
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"start": "npm-run-all --parallel dev:*",
		"build": "run-s prod:*",
		"dev:eleventy": "eleventy --serve",
		"dev:parcel": "parcel watch ./_assets/js/main.js --out-dir ./_site/assets",
		"prod:eleventy": "eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "David Walker",
		"email": "dave@blessedbeyondfoundation.com",
		"url": "https://radicaldave.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.0-beta.2",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.1",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
		"luxon": "^3.2.1",
		"markdown-it-anchor": "^8.6.6",
		"npm-run-all": "^4.1.5"
	},
	"dependencies": {
		"bootstrap": "^5.2.3",
		"cheerio": "^1.0.0-rc.12",
		"fs": "^0.0.1-security",
		"jquery": "^3.6.3",
		"popper.js": "^1.16.1",
	}
}
